{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["title", "runs", "memoTemplatePath", "outDir"],
  "properties": {
    "title": { "type": "string" },
    "runs": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["label", "params", "series"],
        "properties": {
          "label": { "type": "string" },
          "params": { "type": "object", "additionalProperties": { "type": "number" } },
          "series": {
            "type": "object",
            "required": ["t", "y"],
            "properties": {
              "t": { "type": "array", "items": { "type": "number" } },
              "y": { "type": "object", "additionalProperties": { "type": "array", "items": { "type": "number" } } }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      }
    },
    "memoTemplatePath": { "type": "string" },
    "outDir": { "type": "string" }
  },
  "additionalProperties": false
}